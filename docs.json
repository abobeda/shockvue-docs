{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Shockvue Docs",
  "colors": {
    "primary": "#F97216",
    "light": "#07C983",
    "dark": "#F97216"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "smart-edit",
              "editing-examples",
              "prompting-guide",
              "remix-images",
              "prompt-best-practices"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Docs",
          "href": "https://docs.shockvueapp.com/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Shockvue",
          "href": "https://shockvueapp.com",
          "icon": "house"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
